$ElemDcl
$ElemId lenInd7_5
$ElemType LenInd7AndE_Bit
$End_ElemDcl
$ElemDcl
$ElemId lenInd15_1
$ElemType LenInd15AndE_Bit
$End_ElemDcl
$ElemDcl
$ElemId lenInd15_2
$ElemType LenInd15AndE_Bit
$End_ElemDcl
$ElemDcl
$ElemId lenInd15_3
