$TestStepId ts_CalculatePaddingSize( p_LIs: LenInds; p_DataSizeOcts, p_StatusSizeHalfOcts: INTEGER )
$TestStepRef RLCv520/Padding/
$Objective /* Calculate the number of half octets padding required to exactly fill the                     
current PDU. */
$DefaultsRef RLC_Default
$Comment /* This test step is used to calculate the number of half octets padding required                    
to exactly fill the payload within an RLC PDU.                    
                    
In general, the calculation required is as follows:                    
                    
tcv_NumHalfOctetsPadding :=                     
  2 * ( tcv_PayloadSize - ( LI_GroupSizeOcts + p_DataSize ) ) - p_StatusSizeHalfOcts                    
                    
The alternatives used are to determine the size of the LI group.                    
                    
Parameters:                    
p_LIs:                    
The length indicator group to be included within the RLC PDU. The presence or                    
absence of each LI determines how many octets remain in the payload to be used for                    
data, piggybacked status, and padding.                    
                    
p_DataSizeOcts:                    
The number of data octets included in the payload. Acceptable values are                     
0 to tcv_PayloadSize inclusive.                    
                    
p_StatusSizeHalfOcts:                    
The number of half octets used for a piggybacked status PDU. If a piggybacked                    
status PDU is not included in the PDU, then this parameter should be 0.                    
                    
Test case variables required:                    
tcv_PayloadSize must be set to the current payload size in octets.                     
                    
Test case variables affected:                    
tcv_NumHalfOctetsPadding will be set to the number of half octets that must be                    
included to exactly fill an RLC PDU with payload size tcv_PayloadSize, containing the                     
given LI group, the given number of octets of data, and optionally a                     
piggybacked status PDU of the given size. */
$BehaviourDescription
$BehaviourLine
$LabelId 
$Line [0] [ IS_PRESENT( p_LIs.lenInd7_1 ) ]
$Cref 
$VerdictId 
$End_BehaviourLine
$BehaviourLine
$LabelId 
$Line [1] [ IS_PRESENT( p_LIs.lenInd7_2 ) ]
$Cref 
$VerdictId 
$End_BehaviourLine
$BehaviourLine
$LabelId 
$Line [2] [ IS_PRESENT( p_LIs.lenInd7_3 ) ]
$Cref 
$VerdictId 
$End_BehaviourLine
$BehaviourLine
$LabelId 
$Line [3] [ IS_PRESENT( p_LIs.lenInd7_4 ) ]
$Cref 
$VerdictId 
$End_BehaviourLine
$BehaviourLine
$LabelId 
$Line [4] [ IS_PRESENT( p_LIs.lenInd7_5 ) ]
$Cref 
$VerdictId 
$End_BehaviourLine
$BehaviourLine
$LabelId 
$Line [5] ( tcv_NumHalfOctetsPadding := 2 * ( tcv_PayloadSize - ( 5 + p_DataSizeOcts ) ) - p_StatusSizeHalfOcts )
$Cref 
$VerdictId 
$End_BehaviourLine
$BehaviourLine
$LabelId 
$Line [4] [ TRUE ]
$Cref 
$VerdictId 
$End_BehaviourLine
$BehaviourLine
$LabelId 
$Line [5] ( tcv_NumHalfOctetsPadding := 2 * ( tcv_PayloadSize - ( 4 + p_DataSizeOcts ) ) - p_StatusSizeHalfOcts )
$Cref 
$VerdictId 
$End_BehaviourLine
$BehaviourLine
$LabelId 
$Line [3] [ TRUE ]
$Cref 
$VerdictId 
$End_BehaviourLine
$BehaviourLine
$LabelId 
$Line [4] ( tcv_NumHalfOctetsPadding := 2 * ( tcv_PayloadSize - ( 3 + p_DataSizeOcts ) ) - p_StatusSizeHalfOcts )
$Cref 
$VerdictId 
$End_BehaviourLine
$BehaviourLine
$LabelId 
$Line [2] [ TRUE ]
$Cref 
$VerdictId 
$End_BehaviourLine
$BehaviourLine
$LabelId 
$Line [3] ( tcv_NumHalfOctetsPadding := 2 * ( tcv_PayloadSize - ( 2 + p_DataSizeOcts ) ) - p_StatusSizeHalfOcts )
$Cref 
$VerdictId 
$End_BehaviourLine
$BehaviourLine
$LabelId 
$Line [1] [ TRUE ]
$Cref 
$VerdictId 
$End_BehaviourLine
$BehaviourLine
$LabelId 
$Line [2] ( tcv_NumHalfOctetsPadding := 2 * ( tcv_PayloadSize - ( 1 + p_DataSizeOcts ) ) - p_StatusSizeHalfOcts )
$Cref 
$VerdictId 
$End_BehaviourLine
$BehaviourLine
$LabelId 
$Line [0] [ IS_PRESENT( p_LIs.lenInd15_1 ) ]
$Cref 
$VerdictId 
$End_BehaviourLine
$BehaviourLine
$LabelId 
$Line [1] [ IS_PRESENT( p_LIs.lenInd15_2 ) ]
$Cref 
$VerdictId 
$End_BehaviourLine
$BehaviourLine
$LabelId 
$Line [2] [ IS_PRESENT( p_LIs.lenInd15_3 ) ]
$Cref 
$VerdictId 
$End_BehaviourLine
$BehaviourLine
$LabelId 
$Line [3] ( tcv_NumHalfOctetsPadding := 2 * ( tcv_PayloadSize - (6 + p_DataSizeOcts ) ) - p_StatusSizeHalfOcts )
$Cref 
$VerdictId 
$End_BehaviourLine
$BehaviourLine
$LabelId 
$Line [2] [ TRUE ]
$Cref 
$VerdictId 
$End_BehaviourLine
$BehaviourLine
$LabelId 
$Line [3] ( tcv_NumHalfOctetsPadding := 2 * ( tcv_PayloadSize - ( 4 + p_DataSizeOcts ) ) - p_StatusSizeHalfOcts )
$Cref 
$VerdictId 
$End_BehaviourLine
$BehaviourLine
$LabelId 
$Line [1] [ TRUE ]
$Cref 
$VerdictId 
$End_BehaviourLine
$BehaviourLine
$LabelId 
$Line [2] ( tcv_NumHalfOctetsPadding := 2 * ( tcv_PayloadSize - ( 2 + p_DataSizeOcts ) ) - p_StatusSizeHalfOcts )
$Cref 
